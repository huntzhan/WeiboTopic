		CC = gcc
		XX = g++
		CFLAGS = -Wall -o -g
	TARGET = ./VirtualProject
	SOURCES = $(wildcard *.c *.cpp)
	OBJS = $(patsubst %.c,%.o,$(patsubst %.cpp,%.o,$(SOURCES)))
$(TARGET) : $(OBJS)
	$(XX) $(OBJS) -o $(TARGET)  -m64  -O3 -DOS_LINUX  ./lib_zjf/src/libprocess.a     -I ./lib_zjf  -L /usr/lib64/mysql    -lmysqlclient
%.o:%.cpp
	$(XX) $(CFLAGS) -c $< -o $@ -m64  -O3 -DOS_LINUX  ./lib_zjf/src/libprocess.a     -I ./lib_zjf  -L /usr/lib64/mysql    -lmysqlclient
	
